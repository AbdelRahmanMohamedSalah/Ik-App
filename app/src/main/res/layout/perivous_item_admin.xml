<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="4.0dip"
    android:background="@drawable/et_style"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/TextCodePreviousItemAdminActivity"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="6.0sp"
            android:gravity="center"
            android:text="L120"
            android:fontFamily="@font/viga"
            android:textSize="20.0sp" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/TextDatePreviousItemActivity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1.0"
                android:drawableStart="@drawable/ic_calender_black_24dp"
                android:gravity="center"
                android:paddingLeft="10.0dip"
                android:text="12/12/2018"
                android:textSize="18.0sp" />

            <TextView
                android:id="@+id/TextTimePreviousItemActivity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1.0"
                android:drawableStart="@drawable/ic_clock_time_black_24dp"
                android:gravity="center"
                android:paddingLeft="10.0dip"
                android:text="7:00 AM"
                android:textSize="18.0sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/TextRequestMemberId"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="6.0sp"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="عميل من "
                android:textSize="18.0sp"
                android:fontFamily="@font/regular"/>

            <TextView
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="6.0sp"
                android:layout_weight="1.0"
                android:gravity="right"
                android:text="عميل من :"
                android:textSize="18.0sp"
                android:fontFamily="@font/regular"/>
        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/TextPerivierwMemberId"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="6.0sp"
                android:layout_weight="1.0"
                android:gravity="center"
                android:text="معاينه من "
                android:textSize="18.0sp"
                android:fontFamily="@font/regular"/>

            <TextView
                android:id="@+id/TextCodePreviousItemActivity"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_margin="6.0sp"
                android:layout_weight="1.0"
                android:gravity="right"
                android:text="معاينه من :"
                android:textSize="18.0sp"
                android:fontFamily="@font/regular"/>
        </LinearLayout>
    </LinearLayout>
</FrameLayout>